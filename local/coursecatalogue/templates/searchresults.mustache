<div class="card">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Course Name</th>
        <th>Course ID</th>
        <th>Start Date</th>
        <th>Tags</th>
        <th>Instructor</th>
      </tr>
    </thead>
    {{#data}} 
      <tr>
      <td>{{{CourseName}}}</td>
      <td>{{{CourseID}}}</td>
      <td>{{{StartDate}}}</td>
      <td>{{Tags}}</td>
      <td>{{Instructor}}</td>
      <td></td>
      </tr>
    {{/data}}
  </table>
  {{^data}}
  <div class="card-body"><p>There are no results</p></div>
    {{/data}}
</div>
